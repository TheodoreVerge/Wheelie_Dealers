<h1>List your bike!</h1>
<div class="basic_form">
  <%= simple_form_for(@bike) do |f|%>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :brand %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit "List it!", class: "card-button" %>
  <% end %>

</div>
